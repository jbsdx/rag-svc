{
  "name": "rag-svc",
  "version": "0.0.0",
  "scripts": {
    "start": "PORT=3003 npx nodemon --ignore data/ src/server.ts",
    "build": "npx tsc",
    "build-types": "npx tsc --declaration --outDir dist --emitDeclarationOnly",
    "precopy-types-to-svc": "npm run build-types",
    "copy-types-to-svc": "rm -rf ../content-creator-svc/packages/rag-svc/* && cp -r dist/* ../content-creator-svc/packages/rag-svc/",
    "docker": "npm run docker-with-llm",
    "docker-with-llm": "docker compose -f docker-compose.base.yml -f docker-compose.llm.yml up",
    "docker-build": "docker compose -f docker-compose.base.yml -f docker-compose.llm.yml build",
    "version": "conventional-changelog && git add CHANGELOG.md"
  },
  "author": "JB",
  "description": "AI RAG RCP server",
  "types": "dist/server.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@qdrant/js-client-rest": "^1.14.1",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "axios": "^1.10.0",
    "dotenv": "^17.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.10",
    "conventional-changelog": "^7.1.1",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "trpc-ui": "^1.0.15",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  }
}
